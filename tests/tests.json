            "leading_zero_float": {
                "input": "0.123",
                "expected": "0.123"
            },
            "leading_point_float": {
                "input": ".123",
                "expected": "0.123"
            },
            "consecutive_decimals": {
                "input": "1...23",
                "expected": "1.23"
            },
            "non_consecutive_decimals": {
                "input": "1.2.3",
                "expected": "1.23"
            }
        }
    },
    "single_operations": {
        "addition": {
            "input": "1+2=",
            "expected": "3"
        },
        "subtraction": {
            "input": "1-2=",
            "expected": "-1"
        },
        "multiplication": {
            "input": "1*2=",
            "expected": "2"
        },
        "division": {
            "input": "1/2=",
            "expected": "0.5"
        }
    },
    "comprehensive_operations": {
        "successive_operation": {
            "input": "1+1+2=",
