            "input": "+",
            "expected": "0+"
        },
        "evaluation": {
            "input": "=",
            "expected": "0"
        }
    },
    "operands": {
        "integers": {
            "non_negative": {
                "single_digit_non_zero": {
                    "input": "1",
                    "expected": "1"
                },
                "multi_digit_non_zero": {
                    "input": "123",
                    "expected": "123"
                },
                "zero": {
                    "input": "0",
                    "expected": "0"
                },
                "leading_zero": {
                    "input": "0023",
                    "expected": "23"
                },
                "trailing_zero": {
                    "input": "1230",
                    "expected": "1230"
                },
                "all_zero": {
                    "input": "0000",
                    "expected": "0"
                }
            }
        },
        "floats": {
            "decimal": {
                "input": ".",
                "expected": "0."
            },
            "decimal_number": {
                "input": "12.34",
                "expected": "12.34"
            },
            "trailing_decimal": {
                "input": "12.+34.",
                "expected": ["12", "+", "34."]
            },
            "trailing_zero": {
                "input": "12.300+45.600",
                "expected": ["12.3", "+", "45.600"]
