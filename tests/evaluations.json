{
    "single_evaluation": {
        "empty": {
            "input": "=",
            "expected": "0"
        },
        "single_operations": {
            "addition": {
                "input": "1+2=",
                "expected": ["3"]
            },
            "subtraction": {
                "input": "1-2=",
                "expected": "-1"
            },
            "multiplication": {
                "input": "1*2=",
                "expected": "2"
            },
            "division": {
                "input": "1/2=",
                "expected": "0.5"
            }
        },
        "comprehensive_operations": {
            "decimals": {
                "input": "1.1+1.1=",
                "expected": "2.2"
            },
            "multiple_decimals": {
                "input": "1...1+1...1=",
                "expected": "2.2"
            },
            "multiple_operation_keys": {
                "input": "1++++2=",
                "expected": "3"
            },
            "changing_operation_keys": {
                "input": "1+-*2=",
                "expected": "2"
            },
            "division_by_zero": {
                "input": "1/0=",
                "expected": "Infinity"
            }
        },
        "grouping": {
            "input": "3*(1+2)=",
            "expected": "9"
        },
        "advanced_ops": {
            "premature_operation": {
                "input": "++++1*3=",
                "expected": "3"
            },
            "partial_operand": {
                "input": "3*=",
                "expected": "9"
            },
            "missing_operation": {
                "input": "3=",
                "expected": "3"
            },
            "missing_operands": {
                "input": "====",
                "expected": "0"
            }
        },
        "extra_operations": {
            "order_of_ops": {
                "input": "1+3/4+10*2=",
                "expected": "21.75"
            },
            "binary_math_rounding": {
                "input": "0.1+0.2=",
                "expected": "0.3"
            }
        }
    },
    "multiple_evaluations": {
        "operation_repeat": {
            "input": "1+1===",
            "expected": "4"
        },
        "operation_rollover": {
            "input": "1+1+=+=",
            "expected": "8"
        },
        "operation_restart": {
            "input": "1+2=3-4=",
            "expected": "-1"
        }
    }
}